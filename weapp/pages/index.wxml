<view class="page">
  <view class="main">
    <!-- 背景 -->
      <view class="animation-wrapper" >
        <view class="sticky-wrapper" style='height:{{height}}px'>
          <image src='../dist/images/cunji.jpg' style='height:100%;'></image>
        </view>
      </view>
      <!-- 文字 -->
      <view class="section-content flurr" style='height:{{height}}px' >
        <view animation="{{animationData}}">
          <view class="page__hd" >
              <view class="page__title">存己科技</view>
          </view> 
          <view class="page__bd page__bd_spacing">
            <view class="button-sp-area">
              <view class="page__title">Never Stop Exploring.</view>
              <view class="page__title">永远不要停止探索.</view>
            </view>
          </view>
        </view>
      </view>
  </view>

    <!-- 第二页 -->
<scroll-view scroll-y="{{scrollY}}" style="height:{{height}}px" bindscroll="bindscroll" scroll-with-animation="true" upper-threshold="10" lower-threshold="10">
    <!-- 文字层 -->
    <view style="height:{{height}}px;opacity:{{num}}" class="section-content ">
      <!-- 文字层的文本 -->
      <view class="section_body" animation="{{animationData}}">
        <view class="page__hd">
              <view class="page__title">我们能做什么？</view>
        </view> 
        <view class="page__bd_title">网站建设</view>
        <view class="page__bd_title">微信公众号</view> 
        <view class="page__bd_title">微信小程序</view> 
        <view class="page__bd_title">H5页面</view> 
        <view class="page__bd_title">APP/webAPP</view> 
        <view class="page__bd_title">产品/UI设计</view>         
      </view>
      <!-- 文字层中的背景图片 -->
      <view class="section_img" >
        <image style='height:{{height}}px;width:{{width}}px' src='../dist/images/back.jpg'></image>
      </view>
    </view>
    <!-- 背景动画 -->
    <view>
      <image src='../dist/images/cunji.jpg' style="width:{{width}}px;"></image>
    </view>
    
</scroll-view>


